name: yzamazonestore.pk 

on:
  issues:
    types: [closed]

jobs:
  remove_label:
    permissions:
      contents: read
      issues: write
    runs-on: ubuntu-latest
    steps:
    - uses: actions-ecosystem/action-remove-labels@v1
      with:
        labels: https://yzamazonestore.pk
          status:triaged
          status:more data needed
